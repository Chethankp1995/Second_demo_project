job ('job dsl example') {
    description('My first DSl job')
    parameters {
        booleanParam('FLAG', true)
        choiceParam('OPTION', ['chethan (default)', 'kp', 'shetty'])
        stringParam('EMAIL', 'chethan@gmail.com', 'my description')
    }
  scm {
        github('https://github.com/chethan710/Second_demo_project.git','master')
    }
  
 triggers {
        cron('H 6 * * 7')
    }
 triggers {
        scm('@daily')
    }
  
  steps {
        shell("echo 'Hello World'")
    }
  
}
